<size>18</size><family>Serif</family>{Model VI: Multistep inhibition of BACH1 by heme}
;Unit: time = second, concentration = nM

METHOD STIFF
STARTTIME = 0
STOPTIME = 3600 * 100
DT = 0.02
Minute = Time / 60

{Parameters}
Heme = 1
k0 = 20 * logn(2) / (100 * 60)
k1 = 0.1 * logn(2) / (0.2 * 60)
k2 = logn(2) / (20 * 60)
k3 = logn(2) / (100 * 60)
k30 = logn(2) / (360 * 60)
k4 = 0.01
k5 = 0.01
k6 = logn(2) / (100 * 60)
k60 = logn(2) / (360 * 60)
k7 = logn(2) / (100 * 60)
k70 = logn(2) / (360 * 60)

{Inintial condition}
init BACH1c = 10
init BACH1n = 10
init ARE = 1
init BACH1nARE = 0

{To enable or disable signaling steps}
Switch1 = 1;
Switch2 = 1;   
Switch3 = 1;   

{ODEs}
BACH1c' = k0 + k1 * (Heme * Switch1 + 1 - Switch1)  * BACH1n - k2 * BACH1c - k3 *  (Heme * Switch3 + 1 - Switch3)  * BACH1c - k30 * BACH1c
BACH1n' = - k1 *  (Heme * Switch1 + 1 - Switch1)  * BACH1n + k2 * BACH1c - k4 * BACH1n * ARE + k5 *  (Heme * Switch2 + 1 - Switch2)  * BACH1nARE - k6 *  (Heme * Switch3 + 1 - Switch3)  * BACH1n - k60 * BACH1n
ARE' = - k4 * BACH1n * ARE + k5 * (Heme * Switch2 + 1 - Switch2) * BACH1nARE + k7 * (Heme * Switch3 + 1 - Switch3)  * BACH1nARE + k70 * BACH1nARE
BACH1nARE' = k4 * BACH1n * ARE - k5 * (Heme * Switch2 + 1 - Switch2)  * BACH1nARE - k7 *  (Heme * Switch3 + 1 - Switch3)   * BACH1nARE - k70 * BACH1nARE

{Composite variables}
Total_BACH1 = BACH1c + BACH1n + BACH1nARE


















