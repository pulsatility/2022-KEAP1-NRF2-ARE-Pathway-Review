<size>18</size><family>Serif</family>{Model IV: Positive autoregulation of NRF2 via transcriptional induction of p62}
;Unit: time = second, concentration = nM

METHOD STIFF
STARTTIME = 0
STOPTIME = 3600 * 400
DT = 0.02
Minute = Time / 60
Hour = Minute / 60

{Parameters}
S 		= 0
k1 		= 0.00135071
k2		= 1.87337e-4
k20 		= 1 *  logn(2) / (100 * 60)
k3 		= 3.30834e-5
k4 		=  logn(2) / (6 * 60 * 60)
k5 		= 1.31138e-4
k6 		= logn(2) / (13 * 60 * 60)
k7 		= 0.1
k8		= 0.1
k9 		= logn(2) / (6 * 60 * 60) 
Kd2 		= 1

{Inintial condition}
init NRF2 	= 1
init p62 		= 1
init KEAP1 	= 5
init KEAP1_p62 	= 1

{To enable or disable p62 induction by NRF2}
Switch1 = 1

{ODEs}
NRF2' = k1 - k2 * KEAP1 * Kd2 / (Kd2 + S) * NRF2 - k20 * NRF2
p62' = k3 * (1 - Switch1 + Switch1 * NRF2) - k4 * p62 - k7 * KEAP1 * p62 + k8 * KEAP1_p62
KEAP1' =  k5 - k6 * KEAP1 - k7 * KEAP1 * p62 + k8 * KEAP1_p62 
KEAP1_p62' =  k7 * KEAP1 * p62 - k8 * KEAP1_p62 - k9 * KEAP1_p62 

{Composite variables}
KEAP1_tot = KEAP1 + KEAP1_p62
p62_tot = p62 + KEAP1_p62








