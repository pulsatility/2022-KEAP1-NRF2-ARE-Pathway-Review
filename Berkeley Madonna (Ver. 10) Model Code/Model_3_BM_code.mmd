<size>18</size><family>Serif</family>{Model III: Positive autoregulation of NRF2 and sMaf via transcriptional self-induction}
;Unit: time = second, concentration = nM

METHOD STIFF
STARTTIME = 0
STOPTIME = 3600 * 100
DT = 0.02
Minute = Time / 60

{Parameters}
S = 0
k10 = 7 * logn(2) / (10 * 60)
k1 = 65 * logn(2) / (10 * 60)
k2 = logn(2) / (10 * 60)
k3 = logn(2) / (100 * 60)
k40 = 6 * logn(2) / (100 * 60)
k4 = 83 * logn(2) / (100 * 60)
k5 = logn(2) / (100 * 60)
k6 = logn(2) / (100 * 60)
k7 = 0.001
k8 = 0.1
Kd1 = 20
Kd2 = 1.5
Kd4 = 20

{Inintial condition}
init NRF2 = 1
init sMAF = 10
init NRF2_sMAF = 1

{ODES}
NRF2' = k10 + k1 * NRF2_sMAF / (Kd1 + NRF2_sMAF) - k2 * Kd2 / (Kd2 + S) * NRF2 - k3 * NRF2 - k7 * NRF2 * sMAF + k8 * NRF2_sMAF
;To disable NRF2 auto-induction
;NRF2' = k10 + k1 * 1 / (Kd1 + 1) - k2 * Kd2 / (Kd2 + S) * NRF2 - k3 * NRF2 - k7 * NRF2 * sMAF + k8 * NRF2_sMAF

sMAF' = k40 + k4 * NRF2_sMAF / (Kd4 + NRF2_sMAF) - k5 * sMAF  - k7 * NRF2 * sMAF + k8 * NRF2_sMAF
;To disable sMaf auto-induction
;sMAF' = k40 + k4 * 1 / (Kd4 + 1) - k5 * sMAF  - k7 * NRF2 * sMAF + k8 * NRF2_sMAF

NRF2_sMAF' = - k6 * NRF2_sMAF + k7 * NRF2 * sMAF - k8 * NRF2_sMAF

{Composite variables}
NRF2_tot = NRF2 + NRF2_sMAF
sMAF_tot = sMAF + NRF2_sMAF















