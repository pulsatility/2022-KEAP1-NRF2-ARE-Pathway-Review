<size>18</size><family>Serif</family>{Model I: KEAP1-dependent NRF2 sequestration and degradation}
;Unit: time = second, concentration = nM

METHOD STIFF
STARTTIME = 0
STOPTIME= 108000
DT = 0.02
DTOUT = 1
MINUTE = TIME / 60

{Parameters}
S 	= 0
k0 	= 0.15
kf 	= 0.01
kb 	= 0.01	
k1 	= 2.888E-4
k2 	= 1.775E-3
Kd1 	= 7
KEAP1_tot = 200

{Inintial condition}
init NRF2 	= 0
init KEAP1 	= KEAP1_tot
init KEAP1_NRF2 = 0

{ODEs}
NRF2' 		= k0 - kf*KEAP1*NRF2 + kb*KEAP1_NRF2 - k1*NRF2
KEAP1' 		= - kf*KEAP1*NRF2 + kb*KEAP1_NRF2 + Kd1/(Kd1+S)*k2*KEAP1_NRF2
KEAP1_NRF2' 	= kf*KEAP1*NRF2 - kb*KEAP1_NRF2 - Kd1/(Kd1+S)*k2*KEAP1_NRF2

{Composite variables}
NRF2tot = NRF2 + KEAP1_NRF2

{Fluxes}
Flux_kf = kf*KEAP1*NRF2
Flux_kb = kb*KEAP1_NRF2
Flux_k1 = k1*NRF2
Flux_k2 = k2*KEAP1_NRF2



